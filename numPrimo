
object numPrimo{

  def verifPrimo(x: Int): Unit = {
    println("ENTROU se primo")
    def div(x: Int, n: Int = x-1): Unit = { 
      if((x % n) !=0) div(x, x-1)
      else if(n == 1) println("É primo!!")
      else println("NÃO é primo!!")
      }
    }
    
  def main(args: Array[String]): Unit = {
    println("Verificando se primo")
    verifPrimo(Array(1))
  }
}
